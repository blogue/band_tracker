<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>@Model.GetName()</title>
  </head>
  <body>
    <h1>@Model.GetName()</h1>
      <h3>Bands that have played here:</h3>
      @foreach(var band in Model.GetBands())
      {
        <li><a href="/bands/@band.GetId()">@band.GetName()</a></li>
      }
  </body>
</html>
